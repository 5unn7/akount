  "_meta": {
    "models": {
      "invoices": "A bill sent to a client requesting payment, containing header information like issue date, due date, total amount, currency, payment status, and links to the client and entity.",
      "invoiceLines": "Individual line items on an invoice, including description, quantity, unit price, linked category or GLAccount, applicable tax codes, and calculated amounts.",
      "bills": "An incoming invoice from a vendor requesting payment, containing similar structure to Invoice but on the payable side.",
      "billLines": "Individual line items on a vendor bill, parallel to InvoiceLine for tracking purchased goods and services.",
      "clients": "A customer who receives invoices and makes payments for services or products delivered. Tracks contact information, payment terms, and account status.",
      "vendors": "A supplier or service provider from whom the entity purchases goods or services. Mirror of Client but for accounts payable.",
      "creditNotes": "Negative invoices or credits that can be applied to existing invoices or refunded, supporting adjustments and returns for both receivables and payables.",
      "payments": "Records money received from clients or paid to vendors, including payment date, amount, currency, payment method (card, transfer, cash), and supports partial payments and overpayments.",
      "paymentAllocations": "Junction entity linking payments to specific invoices or bills, supporting scenarios where one payment settles multiple invoices and enabling proper aging reports.",
      "products": "Reusable line item templates with name, description, default price, and tax rate for quick invoice and bill creation."
    },
    "relationships": [
      "Each Invoice belongs to a Client and contains one or more InvoiceLines",
      "Each Bill belongs to a Vendor and contains one or more BillLines",
      "Each Payment can be allocated to multiple Invoices or Bills via PaymentAllocations",
      "CreditNotes link to specific Invoices or Bills to adjust amounts",
      "Products serve as templates for creating InvoiceLines and BillLines"
    ]
